[sub_resource type="GDScript" id="GDScript_0dkrt"]
script/source = "extends PopupMenu


func _ready() -> void:
	id_pressed.connect(func(id: int):
		match id:
			0:
				ThemeManager.reload_theme()
			1:
				set_item_checked(id, not is_item_checked(id))
				DebugManager.show_bounds = is_item_checked(id)
			2:
				set_item_checked(id, not is_item_checked(id))
				DebugManager.giant_brush_deadzone = is_item_checked(id)
	)
"

